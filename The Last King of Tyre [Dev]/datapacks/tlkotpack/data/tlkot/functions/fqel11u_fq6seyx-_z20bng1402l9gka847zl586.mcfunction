tellraw @a {"text": "Ohhhh...", "color": "dark_gray", "italic": true}
tellraw @a {"text": "Another undead has come, come seeking freedom, come seeking death.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "Well then, I... No.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "No no, what kind of abomination are you?", "color": "dark_gray", "italic": true}
tellraw @a {"text": "Your heart already belongs to me!", "color": "dark_gray", "italic": true}
tellraw @a {"text": "Hm...", "color": "dark_gray", "italic": true}
tellraw @a {"text": "So.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "You fought my evangelists.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "You experienced my rapturous Daugh communion, and yet you resist.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "...You're here to activate the former minister's resonating crystal.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "...", "color": "dark_gray", "italic": true}
tellraw @a {"text": "I cannot allow that.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "The purge must remain a myth.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "Your journey ends here, monster.", "color": "dark_gray", "italic": true}
tellraw @a {"text": "I am the \u00a7lSaviour\u00a78\u00a7o, \u00a7lI\u00a78\u00a7o will not be killed...", "color": "dark_gray", "italic": true}
tellraw @a {"text": "Not be \u00a7loverruled\u00a78\u00a7o, not be \u00a7ltrivialised\u00a78\u00a7o, not by you, not by \u00a7lANYONE\u00a78\u00a7o!", "color": "dark_gray", "italic": true}